if [ $# -eq 0 ] ; then
    echo "--
Param #1 domain (zone) is empty, param #2 host is empty
Correct syntax: dns-server domain.com 12.23.34.45
--"
    exit 1
fi
if [ $# -eq 1 ] ; then
    echo "--
Param #2 host is empty
Correct syntax:: dns-server domain.com 12.23.34.45
--"
    exit
fi
#php ~/ngn-env/run/run.php '(new DnsServer)->replaceZone("$1", "$2")' NGN_ENV_PATH/dns-server/lib